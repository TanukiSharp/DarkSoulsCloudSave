<Window x:Class="DarkSoulsCloudSave.CloudStorageSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DarkSoulsCloudSave"
        mc:Ignorable="d"
        Title="CloudStorageSelector" Height="300" Width="300" Background="WhiteSmoke" Icon="Resources/Game.png" WindowStartupLocation="CenterOwner">
    <ContentControl Content="{Binding}" ContentTemplate="{StaticResource CloudStorageSelectorView}" Focusable="False"/>
</Window>
