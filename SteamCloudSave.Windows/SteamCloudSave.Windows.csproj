    <Resource Include="Resources\Game.png" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SteamCloudSave.Core\SteamCloudSave.Core.csproj" />
    <ProjectReference Include="..\SteamCloudSave.DropboxExtension\SteamCloudSave.DropboxExtension.csproj" />
    <ProjectReference Include="..\SteamCloudSave.GoogleDriveExtension\SteamCloudSave.GoogleDriveExtension.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
