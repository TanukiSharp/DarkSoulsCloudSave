    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
	<AssemblyVersion>1.0.8</AssemblyVersion>
    <ApplicationIcon>Icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <AppDesigner Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
